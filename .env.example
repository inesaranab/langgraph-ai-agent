# Environment Variables for LangGraph AI Agent App

# OpenAI API Key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Tavily Search API Key (required for web search)
TAVILY_API_KEY=your_tavily_api_key_here

# LangSmith (optional, for tracing and evaluation)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=langgraph-agent-app

# App Configuration
APP_TITLE="LangGraph AI Agent"
APP_DESCRIPTION="Intelligent AI agent powered by LangGraph"
DEBUG=false

# FastAPI Configuration  
FASTAPI_SERVER_PORT=8000
FASTAPI_SERVER_ADDRESS=0.0.0.0

# Frontend Configuration (for development)
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000